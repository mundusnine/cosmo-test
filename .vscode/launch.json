{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug app",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/Deployment/calc.com.dbg",
        "args": ["--strace"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/Deployment",
        "environment": [],
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "Build app"
      },
      {
        "name": "Debug knob.com",
        "type": "cppdbg",
